{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "react-hooks/exhaustive-deps": "off",
    "indent": ["error", 4],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "max-len": ["error", { "code": 100, "ignoreComments": true, "ignoreStrings": true }]
  }
}

